{% extends 'base.html.twig' %}

{% block body %}
    <h4><a href="{{ path('artist_detail', {'artist': item.artist}) }}">{{ item.artist }}</a> - {{ item.title }}</h4>
    <h5>
        {{ item.format }} - <a href="{{ path('label_detail', {'label': item.label}) }}">{{ item.label }}</a> - {{ item.catno }}</h5>
    {% if item.comments != null  %}
        <p>( {{ item.comments }} )</p>
    {% endif %}
    <p>release Id : {{ item.releaseId }}</p>
    <p>listing Id : {{ item.listingId }}</p>
    <p>status : {{ item.status }} ( â‚¬{{ item.price | number_format}} )</p>
{% endblock %}

{% block title %}
    {{ parent() }} | {{ item.title }}
{% endblock %}

